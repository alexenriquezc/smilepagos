{
  "name": "smile/smilepagos",
  "description": "Smile PrestaShop Payments module",
  "homepage": "https://github.com/klebeer/prestashop-datafast",
  "authors": [
    {
      "name": "Kleber Ayala",
      "email": "dev@null.ec"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "nategood/httpful": "*",
    "monolog/monolog": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "smile\\payment\\": "src/classes/"
    },
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false
  },
  "type": "prestashop-module"
}
